<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/RelativeLayout1"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:paddingBottom="@dimen/known_list_padding_bottom"
                android:paddingLeft="@dimen/known_list_padding_left"
                android:paddingTop="@dimen/known_list_padding_top"
        >

    <ImageView
            android:id="@+id/NETWORK_ICON"
            android:layout_width="@dimen/known_list_icon_width"
            android:layout_height="@dimen/known_list_icon_height"
            android:layout_alignParentRight="true"
            android:layout_alignTop="@+id/ssid"
            android:layout_marginRight="18dp"
            android:gravity="right"
            android:src="@drawable/buttons"
            android:focusable="false"/>

    <TextView
            android:id="@+id/ssid"
            style="@style/Wftext.large"
            android:layout_width="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginTop="15dp"
            android:gravity="center_horizontal"
            android:text="EMPTY"
            android:focusable="false"/>

</RelativeLayout>