<preference-headers xmlns:android="http://schemas.android.com/apk/res/android" >

    <header
        android:fragment="org.wahtod.wififixer.ui.GenericPreferenceFragment"
        android:icon="@drawable/ic_menu_manage"
        android:key="prefscreen"
        android:title="Service" >
        <extra
            android:name="resource"
            android:value="general" />
    </header>
    <header
        android:fragment="org.wahtod.wififixer.ui.GenericPreferenceFragment"
        android:icon="@drawable/ic_menu_notifications"
        android:key="prefscreen"
        android:title="Notification" >
        <extra
            android:name="resource"
            android:value="notification" />
    </header>
    <header
        android:fragment="org.wahtod.wififixer.ui.GenericPreferenceFragment"
        android:icon="@drawable/cog"
        android:key="prefscreen"
        android:title="Widget" >
        <extra
            android:name="resource"
            android:value="widget" />
    </header>
    <header
        android:fragment="org.wahtod.wififixer.ui.GenericPreferenceFragment"
        android:icon="@drawable/ic_menu_save"
        android:key="prefscreen"
        android:title="Logging" >
        <extra
            android:name="resource"
            android:value="logging" />
    </header>
    <header
        android:fragment="org.wahtod.wififixer.ui.GenericPreferenceFragment"
        android:icon="@drawable/ic_menu_add"
        android:key="prefscreen"
        android:title="Advanced" >
        <extra
            android:name="resource"
            android:value="advanced" />
    </header>

</preference-headers>